  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "run-p build:server build:client",
    "build:server": "tsc --project config/tsconfig.server.json",
    "build:client": "webpack --config ./config/webpack.prod.js",
    "heroku-postbuild": "node ./scripts/get-records.js && yarn build",
    "postbuild": "mkdir lib/views && cp src/server/views/* lib/views",
    "dev": "run-p dev:server dev:client",
    "dev:server": "nodemon -r dotenv/config src/server/index.ts",
    "dev:client": "node -r dotenv/config node_modules/.bin/webpack-dev-server --config ./config/webpack.dev.js",
    "lint": "tslint -c ./tslint.json -p tsconfig.json -e **/node_modules -e src/custom-typings/**/*.ts",
    "start": "node -r dotenv/config lib/index.js",
    "test": "run-s test:server test:client",
    "test:client": "node -r ./src/client/init-test-env.js node_modules/.bin/karma start --browsers ChromeHeadless --single-run --progress=false",
    "test:client:watch": "node -r ./src/client/init-test-env.js node_modules/.bin/karma start --browsers ChromeWithDebugging --autoWatch",
    "test:server": "mocha --opts mocha.opts $(find src/server/__test__ -name '*.ts')",
    "test:server:watch": "yarn test:server --watch"
  },
